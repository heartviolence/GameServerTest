{
  "MagicOnion": {
    "ServerPorts": [
      {
        "Host": "0.0.0.0",
        "Port": 7777,
        "UseInsecureConnection": false,
        "ServerCredentials": [
          {
            "CertificatePath": "./Cert/local_server.crt",
            "KeyPath": "./Cert/local_server.key"
          }
        ]
      }
    ]
  },
  "Kestrel": {
    "EndPoints": {
      "HttpsFromPem": {
        "Url": "https://0.0.0.0:7777",
        "Certificate": {
          "Path": "./Cert/local_server.crt",
          "KeyPath": "./Cert/local_server.key"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RedisConnectionString": "localhost:6379",
  "AllowedHosts": "*"
}
